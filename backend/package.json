    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "inserturlparams": "^2.0.5",
    "jet-env": "^1.1.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.1.0",
    "jet-validators": "^1.4.3",
    "jsonfile": "^6.2.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.1",
    "openai": "^5.23.1",
    "pino": "^9.11.0",
    "pino-http": "^10.5.0",
    "zod": "^4.1.11"
